			Switch["2a"]["BackgroundTransparency"] = 1;
			Switch["2a"]["BorderColor3"] = Color3.fromRGB(0, 0, 0);
			Switch["2a"]["Position"] = UDim2.new(0.94, 0, 0.201, 0);
			
			Switch["2a"].Activated:Connect(function()
				SwitchState = not SwitchState

				if SwitchState == true then
					Switch["28"].BackgroundColor3 = Color3.fromRGB(109, 136, 84)
				else
					Switch["28"].BackgroundColor3 = Color3.fromRGB(100, 100, 100)
				end
				
				SwitchConfig["Callback"](SwitchState)
			end)
			
			function Switch:GetState()
				return SwitchState
			end
			
			return Switch
		end
		
