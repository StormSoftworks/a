			Switch["29"]["CornerRadius"] = UDim.new(0, 2);


			-- StarterGui.StormField.MainFrame.CProv.Tab1.ScrollingFrame.Switch.TextButton
			Switch["2a"] = Instance.new("TextButton", Switch["24"]);
			Switch["2a"]["BorderSizePixel"] = 0;
