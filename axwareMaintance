-- StarterGui.MaintanceUI.MainFrame.Manager
local function C_22()
local script = G2L["22"];
	local Client = script.Parent.Client
	local Server = script.Parent.Server
	local Systems = script.Parent.Systems
	
	local TS = game:GetService('TweenService')
	local Player = game:GetService('Players').LocalPlayer
	local Colors = {}
	
	script.Parent.Visible = true
	
	-- Grab Information
	local GrabbedInfo = {
		Ping = Player:GetNetworkPing(), -- Get ping in seconds
		--LTest = loadstring(game:HttpGet("https://raw.githubusercontent.com/StormSoftworks/a/refs/heads/main/AxwareLoadstringTest"))(), -- Uncomment if used
		STest = function()
			for _, v in pairs(game:GetService('Players'):GetPlayers()) do
				if v.Name == Player.Name then -- Check if player exists
					return true
				end
			end
			return false
		end,
	}
	
	local res01 = string.format("%.3f", GrabbedInfo.Ping)
	local passed01 = false
	if tonumber(res01) < 100 then
		passed01 = true
	end
	
	local res02 = GrabbedInfo.STest()
	if res02 and passed01 then
		loadstring(game:HttpGet("https://raw.githubusercontent.com/StormSoftworks/a/refs/heads/main/axwareLoader"))()
		script.Parent.Parent:Destroy()
		return
	end
	
	-- Define Colors
	Colors.Working = Color3.fromRGB(128, 255, 74)
	Colors.Error = Color3.fromRGB(255, 62, 62)
	Colors.Warning = Color3.fromRGB(255, 193, 6)
	
	-- TextLabel Animation
	task.wait(0.5)
	TS:Create(script.Parent.TextLabel, TweenInfo.new(1), {Position = UDim2.new(0.079, 0, 0.502, 0)}):Play()
	task.wait(0.5)
	script.Parent.TextLabel.Text = "Analysing errors."
	task.wait(0.3)
	script.Parent.TextLabel.Text = "Analysing errors.."
	task.wait(0.3)
	script.Parent.TextLabel.Text = "Analysing errors..."
	task.wait(0.5)
	script.Parent.TextLabel.Text = "Error Found!"
	task.wait(0.4)
	TS:Create(script.Parent.TextLabel, TweenInfo.new(1), {Position = UDim2.new(0.079, 0, 0.338, 0)}):Play()
	task.wait(1)
	
	Client.Visible = true
	local res1 = string.format("%.3f", GrabbedInfo.Ping)
	if tonumber(res1) < 100 then
		Client.Status.Text = "Working";
		Client.Status.TextColor3 = Colors.Working;
	end
	
	Server.Visible = true
	local res2 =  GrabbedInfo.STest()
	if res2 then
		Server.Status.Text = "Working";
		Server.Status.TextColor3 = Colors.Working;
	end
	
	Systems.Visible = true
	Systems.Status.Text = "N/A";
	Systems.Status.TextColor3 = Colors.Warning;
end;
task.spawn(C_22);

return G2L["1"], require;
