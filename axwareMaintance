-- StarterGui.MaintanceUI.MainFrame.Server.LocalScript
local function C_d()
local script = G2L["d"];
	local OffClr = Color3.fromRGB(63, 63, 63)
	local OnClr = Color3.fromRGB(51, 117, 168)
	local TS = game:GetService('TweenService')
	
	script.Parent.MouseEnter:Connect(function()
		TS:Create(script.Parent.UIStroke, TweenInfo.new(.5), {Color = OnClr}):Play()
	end)
	script.Parent.MouseLeave:Connect(function()
		TS:Create(script.Parent.UIStroke, TweenInfo.new(.5), {Color = OffClr}):Play()
	end)
end;
task.spawn(C_d);
-- StarterGui.MaintanceUI.MainFrame.Client.LocalScript
